    "babel-plugin-module-resolver": "^5.0.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "react-native-web": {
      "react": "18.2.0",
      "react-dom": "18.2.0"
    }
  }
