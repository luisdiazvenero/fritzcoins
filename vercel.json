{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "EXPO_ROUTER_APP_ROOT": "app",
    "NODE_ENV": "production",
    "BABEL_ENV": "production",
    "EXPO_USE_WEBPACK": "true"
  }
}
